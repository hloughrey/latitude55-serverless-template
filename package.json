{
  "name": "latitude55-serverless-template",
  "version": "0.0.1",
  "private": true,
  "description": "Latitude55 Serverless Template",
  "main": "serverless.ts",
  "engines": {
    "node": ">= 14.17 < 15"
  },
  "scripts": {
    "build": "sls package",
    "ci:deploy": "sls deploy --aws-s3-accelerate",
    "ci:remove": "sls remove",
    "ci:configure": "sls config credentials",
    "remove": "sls remove --stage",
    "format": "prettier --check \"**/*.ts\"",
    "format:fix": "prettier --write \"**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "start": "sls offline start",
    "test": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "test:cov": "jest --runInBand --coverage",
    "lint-staged": "lint-staged",
    "doctoc": "doctoc ./README.md",
    "prepare": "husky install"
  },
  "dependencies": {
    "got": "11.8.3",
    "luxon": "2.3.1",
    "md5": "2.3.0",
    "pg": "8.7.3",
    "pg-format": "1.0.4",
    "uuid": "8.3.2",
    "winston": "3.6.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.71",
    "@types/jest": "27.4.0",
    "@types/node": "14.14.25",
    "@types/pg-format": "1.0.2",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "ajv": "8.10.0",
    "doctoc": "2.1.0",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-unicorn": "41.0.0",
    "fork-ts-checker-webpack-plugin": "7.2.1",
    "husky": "7.0.0",
    "jest": "27.5.1",
    "json-schema-to-ts": "1.5.0",
    "lint-staged": "12.3.4",
    "nock": "13.2.4",
    "prettier": "2.5.1",
    "serverless": "3.2.1",
    "serverless-dotenv-plugin": "3.12.2",
    "serverless-offline": "8.5.0",
    "serverless-plugin-aws-alerts": "1.7.4",
    "serverless-plugin-canary-deployments": "0.7.1",
    "serverless-webpack": "5.6.0",
    "supertest": "6.2.2",
    "ts-jest": "27.1.3",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.1.3",
    "webpack": "5.69.1"
  },
  "volta": {
    "node": "14.17.6",
    "npm": "6.14.16"
  }
}
